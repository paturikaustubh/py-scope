{
  "name": "py-braces",
  "displayName": "PyBraces - Python Curly Braces Visualization",
  "description": "Enhance Python code readability with visual pseudo-curly braces and block highlighting. Perfect for developers transitioning from C-style languages.",
  "version": "0.0.1",
  "publisher": "Kaustubh Paturi",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages",
    "Formatters",
    "Visualization",
    "Other"
  ],
  "keywords": [
    "python",
    "code structure",
    "curly braces",
    "code visualization",
    "readability",
    "syntax enhancement",
    "block highlighting",
    "developer tools",
    "python indentation",
    "python block",
    "code highlighting"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "PyBraces"
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.24.0",
    "eslint": "^9.16.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pasturikaustubh/py-braces"
  },
  "bugs": {
    "url": "https://github.com/pasturikaustubh/py-braces/issues"
  },
  "homepage": "https://github.com/pasturikaustubh/py-braces#readme",
  "icon": "images/icon.png",
  "license": "MIT",
  "dependencies": {
    "py-braces": "file:"
  }
}
